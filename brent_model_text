   "source": [
    "def get_eikon(tickers, start_date):\n",
    "    fields=['CLOSE','OPEN','LOW','HIGH']\n",
    "    fn = lambda x: ek.get_timeseries(x[1], start_date=start_date, interval='daily',fields=fields)[fields].rename(x[0])\n",
    "    return pd.concat(map(fn, tickers.items()), axis=1).resample('D').median().dropna(axis=0)"
   ]
