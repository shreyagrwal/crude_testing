   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "### Rolling correlations"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 58,
   "metadata": {},
   "outputs": [],
   "source": [
    "def rolling_correlation(data, window, year):\n",
    "    df = data.copy()\n",
    "    rolling_corr = df.brent.rolling(window).corr(df[df.columns[1:]])\n",
    "    rolling_corr.loc[year:].plot(subplots=True, figsize=(8,8))\n",
    "    plt.title(label=F'Rolling {int(window/5)} week correlation', y=8.5)"
   ]
  },
