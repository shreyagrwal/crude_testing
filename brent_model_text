  {
   "cell_type": "code",
   "execution_count": 60,
   "metadata": {},
   "outputs": [],
   "source": [
    "def rolling_params(data, freq, year, window):\n",
    "    df = data.loc[year:].fillna('ffill').resample(freq).last().diff().dropna()\n",
    "    endog = df.brent.to_numpy(dtype='float')\n",
    "    exog = df[df.columns[1:]].to_numpy(dtype='float')\n",
    "    mod = RollingOLS(endog, exog, window=window)\n",
    "    result = mod.fit()\n",
    "    rolling_params = pd.DataFrame(result.params)\n",
    "    rolling_params.columns = df.columns[1:]\n",
    "    rolling_params.index = df.index\n",
    "    rolling_params.plot(subplots=True, figsize=(8,8))\n",
    "    plt.title(label=F'Contribution of factors (rolling {window} day regression)', y=8.5)\n",
    "    return "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 61,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 800x800 with 8 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "rolling_params(data, 'B', '2021', 60)"
   ]
  },
